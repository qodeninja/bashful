#!/usr/bin/env bash

  source "./lib/interface.sh";

  trap exit_signal EXIT

  # if same_file ./setup /Users/qodeninja/setup-bash; then
  #   echo 'same file'
  # else
  #   echo 'dunno!'
  # fi


  case "$1" in
      welcome)
        bashful_welcome "Bashful"
        ;;
      check)
        bashful_config
        ;;
      help)
        echo 'lets install something yay'
        ;;
      auto*|install*)
        bashful_config
        bashful_install
        ;;
      clean)
        echo 'lets install something yay'
        ;;
      *)
    echo $"Usage: install | config"
    exit 1
  esac
  exit $?

